{
  "name": "gis-viewer",
  "version": "0.0.8b32",
  "description": "Stencil Component Starter",
  "main": "dist/gisviewer.js",
  "types": "dist/types/components.d.ts",
  "files": [
    "dist/",
    "dist/collection"
  ],
  "collection": "dist/collection/collection-manifest.json",
  "browser": "dist/gisviewer.js",
  "scripts": {
    "build": "stencil build",
    "release": "npm run build && npm publish",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "dependencies": {
    "html2canvas": "1.0.0-alpha.8",
    "id-generator": "1.1.0",
    "leaflet": "1.3.1",
    "leaflet-draw": "1.0.2",
    "leaflet-fullscreen": "1.0.2",
    "leaflet-minimap": "3.6.1",
    "leaflet-search": "2.7.2",
    "leaflet.heat": "0.2.0",
    "leaflet.markercluster": "1.3.0",
    "leaflet.mousecoordinatesystems": "1.0.2",
    "leaflet.polylinemeasure": "git+https://github.com/ppete2/Leaflet.PolylineMeasure.git",
    "leaflet.styledlayercontrol": "git+https://github.com/iofirag/Leaflet.StyledLayerControl.git",
    "lodash": "4.17.5",
    "mobx": "4.1.1"
  },
  "devDependencies": {
    "@stencil/core": "0.7.26",
    "@stencil/dev-server": "latest",
    "@stencil/sass": "0.0.3",
    "@stencil/utils": "latest",
    "@types/html2canvas": "0.5.34",
    "@types/jest": "21.1.10",
    "@types/leaflet": "1.2.7",
    "@types/leaflet-draw": "0.4.12",
    "@types/leaflet-fullscreen": "1.0.4",
    "@types/leaflet.polylinemeasure": "1.0.0",
    "@types/lodash": "4.14.107",
    "jest": "21.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iofirag/stenciljs-gis-web-component.git"
  },
  "author": "Ionic Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iofirag/stenciljs-gis-web-component"
  },
  "homepage": "https://github.com/ionic-team/stencil",
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
